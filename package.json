{
  "name": "mobx-fields",
  "version": "0.0.9",
  "description": "Mobx form fields builder - create simple to complex form with ease",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf lib && rimraf build",
    "build:watch": "tsc --watch",
    "prettify": "prettier --write \"src/**/*.{js,ts}\"",
    "prettify:check": "prettier -l \"src/**/*.ts\"",
    "tslint": "tslint './src/**/*.{ts,tsx}' --project ./tsconfig.json",
    "lint": "yarn prettify:check && yarn tslint",
    "test": "jest"
  },
  "author": "Adrien Ogor <adrien@iamasoft.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/validator": "^13.1.4",
    "date-fns": "^2.7.0",
    "husky": "^4.2.3",
    "jest": "^27.0.5",
    "mobx": "^5.14.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.1",
    "typescript": "^4.3"
  },
  "peerDependencies": {
    "date-fns": "2",
    "mobx": ">=5"
  }
}
